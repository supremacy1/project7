    font-size: 30px; /* Adjusted font size for smaller screens */
    line-height: 50px; /* Adjusted line height for smaller screens */
}

.second span {
    font-size: 30px; /* Adjusted font size for smaller screens */
    font-weight: 700;
}

.second span h5 {
    font-size: 14px; /* Reduced font size for smaller screens */
}

