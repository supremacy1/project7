/* General styles */
.container {
    position: relative;
    display: flex;
    align-items: center; /* Vertically center the content */
    padding: 10px; /* Reduced padding for smaller screens */
    margin-top: -2%; /* Adjusted margin for smaller screens */
}

.image {
    max-width: 50%; /* Reduced image width for smaller screens */
    height: auto;
    margin-right: 1%; /* Reduced spacing between image and text */
}

.text {
    display: flex;
    position: relative;
    color: #0B132A;
    font-family: Rubik;
    font-size: 16px; /* Reduced font size for smaller screens */
    font-weight: 500;
    line-height: normal;
}

h2 {
    font-size: 20px; /* Adjusted heading font size for smaller screens */
    margin-bottom: 8px; /* Reduced margin for smaller screens */
}

p {
    font-size: 14px; /* Reduced paragraph font size for smaller screens */
    color: #333;
}

/* Buttons */
.buttons {
    display: flex;
    margin-right: 1%; /* Reduced margin for smaller screens */
}

.b1, .b2 {
    font-size: 14px; /* Reduced font size for smaller screens */
}

/* Navigation */
.nav-list {
    list-style: none;
    padding: 0;
    display: flex;
    margin: 0;
    font-size: 14px; /* Reduced font size for smaller screens */
}

